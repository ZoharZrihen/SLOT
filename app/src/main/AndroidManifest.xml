<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.slot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.NoActionBar.Bridge">


        <activity android:name=".student.showAppointmentStudentActivity" />
        <activity android:name=".student.setAppointmentStudentActivity" />
        <activity android:name=".professor.showAppointmentProfessorActivity" />
        <activity android:name=".professor.setAppointmentProfessorActivity" />
        <activity android:name=".student.StudentMainActivity" />
        <activity android:name=".professor.ProfessorMainActivity" />
        <activity android:name=".loginandsignin.SignIn_Activity" />
        <activity android:name=".loginandsignin.SignUpActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>